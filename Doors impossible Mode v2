Creator.runEntity(entity)

local Creator = loadstring(game:HttpGet("https://pastebin.com/raw/txV1ZG7S"))()

-- Create entity
local entity = Creator.createEntity({
    CustomName = "Depth", -- Custom name of your entity
    Model = "rbxassetid:////11524906962", -- Can be GitHub file or rbxassetid
    Speed = 80, -- Percentage, 100 = default Rush speed
    DelayTime = 680, -- Time before starting cycles (seconds)
    HeightOffset = 0,
    CanKill = true,
    KillRange = 20,
    BreakLights = true,
    BackwardsMovement = false,
    FlickerLights = {
        false, -- Enabled/Disabled
        5, -- Time (seconds)
    },
    Cycles = {
        Min = 1,
        Max = 1,
        WaitTime = 3,
    },
    CamShake = {
        true, -- Enabled/Disabled
        {3.5, 20, 0.1, 1}, -- Shake values (don't change if you don't know)
        50, -- Shake start distance (from Entity to you)
    },
    Jumpscare = {
        true, -- Enabled/Disabled
        {
            Image1 = "rbxassetid://11278229112", -- Image1 url
            Image2 = "rbxassetid://11278229112", -- Image2 url
            Shake = true,
            Sound1 = {
                9116390056, -- SoundId
                { Volume = 1 }, -- Sound properties
            },
            Sound2 = {
                5029269312, -- SoundId
                { Volume = 1 }, -- Sound properties
            },
            Flashing = {
                true, -- Enabled/Disabled
                Color3.fromRGB(125, 249, 255), -- Color
            },
            Tease = {
                true, -- Enabled/Disabled
                Min = 1.5,
                Max = 1,
            },
        },
    },
    CustomDialog = {"You died to who you call Depth.", "if you hear swirling hide", "Remember that Depth sometimes waits for you"}, -- Custom death message
})
 
